{
    "name": "endroid/form-bundle",
    "description": "Endroid Form Bundle for Symfony",
    "keywords": ["endroid", "form", "symfony", "bundle"],
    "homepage": "https://github.com/endroid/EndroidFormBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "suggest": {
        "sonata/admin-bundle": "Allows easy form administration"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Bundle\\FormBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Bundle\\FormBundle\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
